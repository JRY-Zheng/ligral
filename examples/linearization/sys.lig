let M = 0.1;
let g = 9.8;
let l = 0.5;
let I = 0.001;

-M*g*l/I*(x1 -> Sin) + @{name:'u'} -> x2d;
[x2; x2d] -> Integrator{name:'x', row:2, col:1} -> [x1; x2];
x1 -> ${name:'y'};

conf lin:
    state:
        x = [0; 0];
    end
    input:
        u = 0;
    end
end